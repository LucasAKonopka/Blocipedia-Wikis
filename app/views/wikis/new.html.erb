<%= form_for @wiki do |f| %>
  
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  
    <%= f.label :body %>
    <%= f.text_area :body, class: 'form-control' %>
    
    
    <div class = "form-group">
     
        <%= f.check_box :private %> Private Wiki
    
    </div>
    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
    </div>
    
  
<% end %>
  